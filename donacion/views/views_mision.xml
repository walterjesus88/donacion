				 		<label for="carta_embajada_consulado" /> 
				 	</group>
				 	<group>
				 		<field name="carta_embajada_consulado"  nolabel="1"  attrs="{'readonly': [('state', 'not in', ('B','01'))]}" widget="binary" filename="name_filename_carta_embajada_consulado"/>
            			<field name="validar_carta_embajada_consulado" readonly="True"  nolabel="1" />
            			<field name="name_filename_carta_embajada_consulado" invisible="1"/>

				 	</group>
				</xpath>				

  				<xpath expr="//group[@name='itinerario_vuelo']" position="inside">
					<group>	
	 					<label for="itinerario_vuelo" />	 					
	 				</group>
					<group>	 					
	 					<field name="itinerario_vuelo"   nolabel="1"  attrs="{'readonly': [('state', 'not in', ('B','01'))]}" widget="binary" filename="name_filename_itinerario_vuelo"/>
            			<field name="validar_itinerario_vuelo" readonly="True" nolabel="1" />            			
            			<field name="name_filename_itinerario_vuelo" invisible="1"/> 
	 				</group>
