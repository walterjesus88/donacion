		            </group>
	          	</xpath>

	          	<xpath expr="//field[@name='cv']" position="after">		     	              
		              <field name="documento_habilitacion" readonly="True"/>     
		              <field name="habilitar" readonly="True"/>

	          	</xpath>

  				<xpath expr="//field[@name='numero_expediente']" position="after">
	          		<field name="name" attrs="{'readonly': [('state', 'not in', ('B','01'))]}"/>
		 			<field name="descripcion" attrs="{'readonly': [('state', 'not in', ('B','01'))]}" />
		 			<field name="fecha_inicio" attrs="{'readonly': [('state', 'not in', ('B','01'))]}"/>
