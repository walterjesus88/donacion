 	<!-- Vistas Heredadas para DONATARIO -->
	    <record id="view_form_mision_donatario" model="ir.ui.view">
	      <field name="name">mision.requisito.donatario.form</field>
	      <field name="model">mision.requisito</field>
	      <field name="inherit_id" ref="view_form_mision_form" />
	      <field name="groups_id" eval="[(4, ref('donacion.group_donacion_requisito_donatario'))]" />
	        <field name="arch" type="xml">

	        	<xpath expr="//page[@name='Observaciones']/group" position="inside">
		        	<field name="observacion_ogcti" readonly="True"/>		
	        	</xpath>

	          	<xpath expr="//page[@name='DIRESA']/group" position="inside">
		            <group>      
