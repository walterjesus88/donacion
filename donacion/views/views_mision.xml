            			<field name="validar_lista_donacion_temporal" readonly="True"  nolabel="1" />	 		
            			<field name="name_filename_lista_donacion_temporal" invisible="1"/>
				 	</group>
				</xpath>
  				
  				<xpath expr="//group[@name='carta_embajada_consulado']" position="inside">
