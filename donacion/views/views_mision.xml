						<field style="text-align:center" name="carta_ogcai" nolabel="1"  attrs="{'readonly': [('state', 'not in', ('B','01'))]}" widget="binary" filename="name_filename_carta_ogcai"/>
            			<field name="validar_carta_ogcai" readonly="True" nolabel="1" />
            			<field name="name_filename_carta_ogcai" invisible="1"/>
			 		</group>
	          	</xpath>

  				<xpath expr="//group[@name='plan_trabajo']" position="inside">
