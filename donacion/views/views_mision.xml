		 		</xpath>

  				<xpath expr="//group[@name='diresas']" position="inside"> 
		 			<field name="user_diresa" context="{'form_view_ref':'donacion.users_readonly'}" domain="['|',('rol_id','=','DIRESA'),('rol_id','=','DISA')]" readonly="True" nolabel="1"/>	        
	          	</xpath>

  				<xpath expr="//group[@name='carta_ogcai']" position="inside">
